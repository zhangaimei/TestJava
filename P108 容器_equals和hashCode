package cn.bjsxt.collection;
/**
 * 根据属性自动生成hashCode(),equals()
 * @author zam
 *
 */
public class Student {
	private int id;
	private String name;
	@Override
	public int hashCode() {
		final int prime = 31;
		int result = 1;
		result = prime * result + id;	//根据id来生成hashCode，生成的hashCode不至于聚集在一块
		return result;
	}
	@Override
	public boolean equals(Object obj) {
		if (this == obj)
			return true;
		if (obj == null)
			return false;
		if (getClass() != obj.getClass())
			return false;
		Student other = (Student) obj;
		if (id != other.id)
			return false;
		return true;
	}
	

}
